{
  "name": "komiku-scraper",
  "version": "1.0.0",
  "description": "Scraper for Komiku manhwa website",
  "main": "api-server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node api/api-server.js",
